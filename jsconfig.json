{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    // "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/ui/*": ["components/ui/*"],
      "@/lib/*": ["lib/*"],
      "@/public/*": ["public/*"],
      "@/styles/*": ["styles/*"],
      "@/context/*":["context/*"],
    },
  }
}
